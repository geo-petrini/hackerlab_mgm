# Immagine docker da usare per gli studenti
IMAGE_NAME=hackerlab:latest

# Prefisso per i container creati
CONTAINER_PREFIX=hlab_

# Porta interna esposta dall'immagine (verr√† pubblicata sull'host)
TARGET_PORT=80

# Range porte host
PORT_MIN=10000
